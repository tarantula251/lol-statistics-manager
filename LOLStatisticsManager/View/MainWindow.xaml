<Window x:Class="LOLStatisticsManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"       
        xmlns:local="clr-namespace:LOLStatisticsManager"
        mc:Ignorable="d"
        Title="League of Legends Statistics Manager" Height="600" Width="800" Background="#FF242337" ResizeMode="NoResize">
    <ScrollViewer VerticalScrollBarVisibility="Auto">
        <Frame x:Name="frame" NavigationUIVisibility="Hidden" Loaded="MainWindow_Loaded"/>
    </ScrollViewer>

</Window>
